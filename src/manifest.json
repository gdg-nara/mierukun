{
  "name": "mierukun",
  "version": "0.1",
  "description": "授業見取りツール",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "popup.js"
        ]
    }
],
  "permissions": [
    "debugger"
  ]
}